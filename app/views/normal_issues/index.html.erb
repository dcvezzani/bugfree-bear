<h1>Listing normal_issues</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @normal_issues.each do |normal_issue| %>
  <tr>
    <td><%= normal_issue.title %></td>
    <td><%= normal_issue.body %></td>
    <td><%= link_to 'Show', normal_issue %></td>
    <td><%= link_to 'Edit', edit_normal_issue_path(normal_issue) %></td>
    <td><%= link_to 'Destroy', normal_issue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Normal issue', new_normal_issue_path %>
